{
  "name": "te1m0z-blog-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "eslint": "npx eslint src --fix",
    "build": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "docs": "npx typedoc --entryPoints \"src/**/*\" --entryPointStrategy expand --out ./docs --plugin typedoc-theme-hierarchy --theme hierarchy"
  },
  "prisma": {
    "schema": "src/prisma/_main.prisma",
    "seed": "ts-node src/prisma/seed.ts"
  },
  "engines": {
    "node": ">=18.17.1",
    "npm": ">=9.6.7"
  },
  "dependencies": {
    "@prisma/client": "5.1.1",
    "@sentry/node": "7.69.0",
    "@sentry/profiling-node": "1.2.1",
    "bcrypt": "5.1.1",
    "body-parser": "1.20.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "date-fns": "2.30.0",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "helmet": "7.0.0",
    "jsonwebtoken": "9.0.2",
    "moment": "2.29.4",
    "prisma": "5.1.1",
    "uuid": "9.0.0",
    "zod": "3.22.2",
    "@faker-js/faker": "8.0.2",
    "@types/bcrypt": "5.0.0",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.13",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.17",
    "@types/express-session": "1.17.7",
    "@types/helmet": "4.0.0",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "20.5.0",
    "@types/uuid": "9.0.3",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "concurrently": "8.2.0",
    "cross-env": "7.0.3",
    "dotenv": "16.3.1",
    "eslint": "8.50.0",
    "morgan": "1.10.0",
    "nodemon": "3.0.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typedoc": "^0.25.1",
    "typedoc-theme-hierarchy": "^4.1.1",
    "typescript": "5.1.6",
    "winston": "3.10.0",
    "winston-daily-rotate-file": "4.7.1"
  }
}
