{
  "name": "te1m0z-blog-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "serve": "NODE_ENV=production node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/server.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only --files -r tsconfig-paths/register src/server.ts",
    "lint": "npx eslint src",
    "eslint": "npx eslint src --fix",
    "prisma:migrate": "npx prisma migrate dev",
    "docs": "npx typedoc --entryPoints \"src/**/*\" --entryPointStrategy expand --out ./docs --plugin typedoc-theme-hierarchy --theme hierarchy",
    "prepare": "husky"
  },
  "prisma": {
    "schema": "prisma/_main.prisma",
    "seed": "ts-node prisma/seed.ts"
  },
  "engines": {
    "node": ">=18.17.1",
    "npm": ">=9.1.0"
  },
  "dependencies": {
    "@faker-js/faker": "8.0.2",
    "@prisma/client": "5.1.1",
    "@sentry/node": "7.69.0",
    "@sentry/profiling-node": "1.2.1",
    "@types/bcrypt": "5.0.0",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.13",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.17",
    "@types/express-session": "1.17.7",
    "@types/helmet": "4.0.0",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "20.5.0",
    "@types/uuid": "9.0.3",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "bcrypt": "5.1.1",
    "body-parser": "1.20.2",
    "compression": "1.7.4",
    "concurrently": "8.2.0",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "date-fns": "2.30.0",
    "dotenv": "16.3.1",
    "eslint": "8.50.0",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "helmet": "7.0.0",
    "jsonwebtoken": "9.0.2",
    "moment": "2.29.4",
    "morgan": "1.10.0",
    "nodemon": "3.0.1",
    "prisma": "5.1.1",
    "redis": "^4.6.14",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typedoc": "^0.25.1",
    "typedoc-theme-hierarchy": "^4.1.1",
    "typescript": "5.1.6",
    "uuid": "9.0.0",
    "winston": "3.10.0",
    "winston-daily-rotate-file": "4.7.1",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "husky": "^9.0.11",
    "module-alias": "^2.2.3",
    "ts-node-dev": "^2.0.0",
    "ttypescript": "^1.5.15",
    "typescript-transform-paths": "^3.4.7"
  }
}
